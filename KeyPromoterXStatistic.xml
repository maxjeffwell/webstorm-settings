<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Save All">
        <Item>
          <IdeaActionID>SaveAll</IdeaActionID>
          <ShortCut>'Ctrl+S'</ShortCut>
          <Description>Save All</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>35</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>Synchronize</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Undo ESLint: Fix 'quotes'">
        <Item>
          <IdeaActionID>$Undo</IdeaActionID>
          <ShortCut>'Ctrl+Z' or 'Alt+Backspace'</ShortCut>
          <Description>Undo ESLint: Fix 'quotes'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open...">
        <Item>
          <IdeaActionID>OpenFile</IdeaActionID>
          <Description>Open...</Description>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>